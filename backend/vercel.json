{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "framework": null,
  "public": false,
  "regions": ["fra1", "iad1", "sfo1"],
  "github": {
    "enabled": true,
    "silent": false
  }
}
